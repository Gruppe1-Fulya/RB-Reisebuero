            </HBox.margin>
            <font>
                <Font name="Raleway Light" size="18.0" />
            </font>
        </Text>
        <ChoiceBox fx:id="addKunde" prefWidth="400.0">
            <HBox.margin>
                <Insets left="140.0" top="10.0" />
            </HBox.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
        </ChoiceBox>
    </HBox>
    <HBox prefHeight="50.0" prefWidth="200.0">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Date:">
            <HBox.margin>
                <Insets left="35.0" top="10.0" />
            </HBox.margin>
            <font>
                <Font name="Raleway Light" size="18.0" />
            </font>
        </Text>
        <TextField fx:id="addDate" prefHeight="20.0" prefWidth="400.0">
            <opaqueInsets>
                <Insets />
