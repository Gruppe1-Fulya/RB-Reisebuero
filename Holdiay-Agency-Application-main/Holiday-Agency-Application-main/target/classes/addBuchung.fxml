            <HBox.margin>
                <Insets left="155.0" top="10.0" />
            </HBox.margin>
        </TextField>
    </HBox>
    <HBox prefHeight="50.0" prefWidth="200.0">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Startzeit:">
            <HBox.margin>
                <Insets left="35.0" top="10.0" />
            </HBox.margin>
            <font>
                <Font name="Raleway Light" size="18.0" />
            </font>
        </Text>
        <TextField fx:id="addStartzeit" prefHeight="20.0" prefWidth="400.0">
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <HBox.margin>
                <Insets left="126.0" top="10.0" />
            </HBox.margin>
        </TextField>
    </HBox>
    <HBox prefHeight="50.0" prefWidth="200.0">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Endezeit:">
